{
    "parser": "babel-eslint",
    "plugins": [
      "import"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "rules": {
      "import/no-unresolved": 2
    },
    "globals": {
        "React": true
    }
}
